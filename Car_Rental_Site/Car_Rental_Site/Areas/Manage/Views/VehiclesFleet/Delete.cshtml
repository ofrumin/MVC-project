    </div>
    <div class="display-label">
        @Html.DisplayNameFor(model => model.TypeID)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.TypeID)
    </div>
   
    

</fieldset>
@using (Html.BeginForm())
{
    <p>
        @Html.HiddenFor(model => model.CarNumber)
        @Html.HiddenFor(model => model.VehicleID)
        @Html.HiddenFor(model => model.TypeID)
        @Html.HiddenFor(model => model.CurrentMileage)
        @Html.HiddenFor(model => model.BranchID)
        @Html.HiddenFor(model => model.Picture)
        @Html.HiddenFor(model => model.Proper)
        
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to Mange", "Index", "MangeHome")
    </p>
}

