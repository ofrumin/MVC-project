<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>VehiclesFleet</legend>

    <div class="display-label">
        @Html.DisplayNameFor(model => model.CarNumber)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CarNumber)
    </div>

    <div class="display-label">
        @Html.DisplayNameFor(model => model.VehicleID)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.VehicleID)
    </div>
    
    <div class="display-label">
        @Html.DisplayNameFor(model => model.CurrentMileage)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CurrentMileage)
    </div>

    <div class="display-label">
        @Html.DisplayNameFor(model => model.Picture)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Picture)
    </div>

    <div class="display-label">
        @Html.DisplayNameFor(model => model.Proper)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Proper)
    </div>
    <div class="display-label">
